https://leetcode.com/problems/matrix-diagonal-sum/submissions/1290297273/
class Solution {
    public int diagonalSum(int[][] mat) {
        int sum=0, row=mat.length, col=mat[0].length;
        for(int i=0, j=0; i<row; i++, j++){
            sum+=mat[i][j];
        }
        for(int i=0, j=col-1; i<row && j>=0; i++, j--){
            if(i==j) continue;
            sum+=mat[i][j];
        }
        return sum;
    }
}
